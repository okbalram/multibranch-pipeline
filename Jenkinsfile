            steps{
                echo 'Starting build'
            }
        }
        stage('Test'){
            steps{
                echo 'Starting Testing'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Starting Deploy'
            }
        }
        stage('Release'){
            steps{
                echo 'Starting Release'
            }
        }
    }
}
